#include <stdio.h>
#include <string.h>

int main()
{
  
    char str[]="computer";
    int i,j,l;
   
    
        l=strlen(str);
        for(i=0;i<l;i++)
        {
            for(j=0;j<=i;j++)
            {
                printf("%c", str[j]);
            }
            printf("\n");
        }
       
    return 0;
}
